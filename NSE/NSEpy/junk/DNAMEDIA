<script type="text/javascript">
var fullURL = "http://www.bullkhan.com/nse/";
var dURL= "http://www.bullkhan.com/"; //"http://www.BullKhan.com/";
var posX=0; var posY=0;
var dtl=0;


function search(){
msb= document.getElementById("msb").value;
msv= document.getElementById("msv").value;
q= "&h=search&v="+msb+":"+msv;
jax( "search", "searchResult", q );
document.getElementById("searchResult").style.border = "#FFFFFF 1px solid";
document.getElementById("searchResult").style.visibility = "visible";
}//func

function hideSearch(){
document.getElementById("searchResult").style.visibility = "hidden";
}//func



function jax( act, outid, q ){
aa= document.getElementById( outid );
u= dURL+act;


var ajaxRequest; try{ ajaxRequest = new XMLHttpRequest(); } catch (e){ try{ ajaxRequest = new ActiveXObject("Msxml2.XMLHTTP"); } catch (e) { try{ ajaxRequest = new ActiveXObject("Microsoft.XMLHTTP"); } catch (e){ alert("Your browser does not support ajax. Allow Active scriptting in internet settings."); return false; } } } ajaxRequest.onreadystatechange= function(){
if(ajaxRequest.readyState == 4){
respo=ajaxRequest.responseText;

if(respo=="login"){ window.location= dURL + "login"; }


aa.innerHTML = respo;

}
}
ajaxRequest.open("POST", u, true);
ajaxRequest.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
ajaxRequest.send(q);
}//func



function addEMA(){
mav= document.getElementById("mav").value;
mac= document.getElementById("mac").value;
q= "h=addEMA&mav="+mav+"&mac="+mac;
jax( "addEMA", "emal", q );
}//func

function delEMA( k ){
q= "h=delEMA&k="+k;
jax( "delEMA", "emal", q );
}//func

function addIndicator(){
n= document.getElementById("indN").value;
v= document.getElementById("indV").value;
q= "&h=addIndicator&n="+n+"&v="+v;
jax(  "addIndicator", "indL", q );
}//func

function delIndicator(n){
q= "&h=delIndicator&n="+n;
jax( "delIndicator", "indL", q );
}//func

function reloadChart(sb, addPara){
dtl=0;
rr= Math.floor(Math.random() * 20);
u=fullURL+"chart/"+sb +"/"+addPara+"/?"+rr;
document.getElementById("IMG").src = u;
//alert(document.getElementById("IMG").src);
}//func


function renko( sb ){
r= document.getElementById("rpf").value;
q= "&h=setRenko&r="+r;
jax( "setRenko", "rpnf", q );
setTimeout(function() { reloadChart(sb,"renko"); }, 3000);
}//func

function pnf( sb ){
r= document.getElementById("rpf").value;
q= "&h=setPNF&r="+r;
jax( "setPNF", "rpnf", q );
setTimeout(function() { reloadChart(sb,"pnf"); }, 3000);
}//func


function dtlON( sb ){
dtl=1;
u=fullURL+"chart/"+sb +"/trendline/250/";
document.getElementById("IMG").src = u;
}//func


function drawTrend( event, img, sb ){
if(dtl==1){
posX=0;
posY=0;
posX = event.offsetX?(event.offsetX):event.pageX-img.offsetLeft;
posY = event.offsetY?(event.offsetY):event.pageY-img.offsetTop;
posX= Math.round(posX);
posY= Math.round(posY);

u=fullURL+"chart/"+sb +"/trendline/250/"+ posX+"/"+posY;
document.getElementById("IMG").src = u;
}//dtl
}//func


function undoLine( sb ){
sec = new Date().getTime();
u=fullURL+"chart/"+sb +"/trendline/250/undo/tt"+sec;
document.getElementById("IMG").src = u;
}

function clearLine( sb ){
sec = new Date().getTime();
u=fullURL+"chart/"+sb +"/trendline/250/clear/tt"+sec;
document.getElementById("IMG").src = u;
}



function scanStocks(n){
u=fullURL+ "scanner/list/";


v=document.getElementById("back").value;
u=u+ "backday-"+ v+"/";


v=document.getElementById("pb").value;
u=u+ "price-"+v+"/";

t=document.getElementById("maba").value;
v=document.getElementById("mav").value;
u=u+ "ma-"+t+"-"+v+"/";

v=document.getElementById("trend").value;
u=u+"trend-"+v+"/";

v=document.getElementById("candle").value;
u=u+ "candle-" +v+ "/";

v=document.getElementById("pattern").value;
u=u+ "pattern-" +v+ "/";

v=document.getElementById("avgVol").value;
u=u+ "vol-" +v+ "/";

v=document.getElementById("moved").value;
u=u+ "moved-" +v+ "/";

window.location=u;
}//func


function tradingCalls(){

t=document.getElementById("t").value;
p=document.getElementById("p").value;

u= fullURL +"tradingCalls/"+t+"/"+p;
window.location=u;
}//func

function clearForm( id ){
document.getElementById(id).value="";
}

function addTrack( sb, q ){

id= "alertL_"+sb;
alertL= document.getElementById(id).value;

id= "notesL_"+sb;
notesL= document.getElementById(id).value;

q = q + "&a="+ alertL+ "&n="+notesL;

id= "ttr_"+sb;
jax( "addTrack", id, q );
}//func

function addToIntraWatch2( sb, q ){

id= "alertL_"+sb;
alertL= document.getElementById(id).value;

id= "notesL_"+sb;
notesL= document.getElementById(id).value;

q = q + "&a="+ alertL+ "&n="+notesL;

id= "ttr_"+sb;
jax( "addToIntraWatch", id, q );
}//func


function addToIntraWatch( m, sb, price, n ){

outid= "iwl_"+sb;
q="&m="+m+"&sb="+sb+"&price="+price+"&n="+n;

jax( "addToIntraWatch", outid, q );

}//func







